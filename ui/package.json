{
	"name": "lazy-mock-ui",
	"version": "1.0.0",
	"description": "lazy-mock-ui",
	"keywords": [],
	"author": "",
	"license": "MIT",
	"scripts": {
		"build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
		"clean-dist": "rimraf dist/*",
		"lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
		"start": "yarn run start-dev",
		"start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
		"start-prod": "yarn run build && node express.js",
		"test": "jest --watch --coverage --config=configs/jest.json",
		"analyz": "webpack-bundle-analyzer --port 8888 ./dist/stats.json",
		"server-build": "rimraf server-dist && tsc -p tsconfig.server.json",
		"server-format": "prettier --write \"nest-server/**/*.ts\"",
		"server-start": "ts-node -r tsconfig-paths/register --project tsconfig.server.json nest-server/main.ts",
		"server-start:dev": "tsc-watch -p tsconfig.server.json --onSuccess \"node server-dist/main.js\"",
		"server-start:debug": "tsc-watch -p tsconfig.server.json --onSuccess \"node --inspect-brk server-dist/main.js\"",
		"server-start:prod": "node server-dist/main.js"
	},
	"dependencies": {
		"@nestjs/common": "^6.0.0",
		"@nestjs/core": "^6.0.0",
		"@nestjs/platform-express": "^6.0.0",
		"antd": "^3.23.6",
		"axios": "^0.19.0",
		"execa": "^2.0.5",
		"express": "^4.17.1",
		"purecss": "^1.0.1",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-router-config": "^5.1.1",
		"react-router-dom": "^5.1.2",
		"reflect-metadata": "^0.1.12",
		"rimraf": "^2.6.2",
		"rxjs": "^6.3.3",
		"serve-static": "^1.14.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.6.0",
		"@babel/core": "^7.6.0",
		"@babel/preset-env": "^7.6.0",
		"@babel/preset-react": "^7.0.0",
		"@nestjs/testing": "^6.0.0",
		"@types/express": "4.16.1",
		"@types/jest": "^24.0.18",
		"@types/node": "^12.7.4",
		"@types/react": "^16.9.2",
		"@types/react-dom": "^16.9.0",
		"@types/supertest": "2.0.7",
		"awesome-typescript-loader": "^5.2.1",
		"babel-loader": "^8.0.6",
		"cross-env": "^6.0.0",
		"css-loader": "^3.2.0",
		"dotenv-webpack": "^1.7.0",
		"file-loader": "^4.2.0",
		"html-webpack-plugin": "^3.2.0",
		"image-webpack-loader": "^6.0.0",
		"jest": "^24.9.0",
		"mini-css-extract-plugin": "^0.8.0",
		"node-sass": "^4.12.0",
		"prettier": "1.17.0",
		"react-hot-loader": "^4.12.12",
		"rimraf": "^3.0.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"supertest": "4.0.2",
		"ts-import-plugin": "^1.6.1",
		"ts-jest": "24.0.2",
		"ts-loader": "^6.2.0",
		"ts-node": "8.1.0",
		"tsc-watch": "2.2.1",
		"tsconfig-paths": "3.8.0",
		"tslint": "^5.19.0",
		"typescript": "^3.6.3",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^4.39.3",
		"webpack-bundle-analyzer": "^3.5.2",
		"webpack-cli": "^3.3.8",
		"webpack-dev-middleware": "^3.7.1",
		"webpack-dev-server": "^3.8.0",
		"webpack-merge": "^4.2.2",
		"webpackbar": "^4.0.0"
	}
}
